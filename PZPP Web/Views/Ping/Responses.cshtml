@model IEnumerable<PZPP_Web.Models.ResponseModel>

@{
    ViewBag.Title = "Responses";
}

<h2>Responses</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Device_Id)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Success)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PingTime)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Time)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Device_Id)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Success)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PingTime)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Time)
        </td>
        <td>            
            @Html.ActionLink("Delete", "DeleteR", new { id=item.Id })
        </td>
    </tr>
}

</table>
